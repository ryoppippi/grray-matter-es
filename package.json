{
  "name": "grray-matter-es",
  "version": "0.0.0",
  "description": "ES-only gray-matter",
  "keywords": [
    "esm",
    "front-matter",
    "gray-matter",
    "yaml"
  ],
  "homepage": "https://github.com/ryoppippi/grray-matter-es#readme",
  "bugs": {
    "url": "https://github.com/ryoppippi/grray-matter-es/issues"
  },
  "license": "MIT",
  "author": "ryoppippi",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryoppippi/grray-matter-es.git"
  },
  "files": [
    "dist",
    "src"
  ],
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.mts",
        "import": "./dist/index.mjs"
      },
      "./package.json": "./package.json"
    }
  },
  "scripts": {
    "build": "tsdown",
    "test": "vitest",
    "lint": "pnpm --aggregate-output run '/^lint:/'",
    "lint:oxlint": "oxlint --max-warnings=0",
    "lint:oxfmt": "oxfmt --no-error-on-unmatched-pattern --check .",
    "format": "pnpm --no-bail --aggregate-output run '/^format:/'",
    "format:oxlint": "oxlint --max-warnings=0 --fix",
    "format:oxfmt": "oxfmt --no-error-on-unmatched-pattern .",
    "prerelease": "pnpm run lint && pnpm run build",
    "release": "bumpp"
  },
  "devDependencies": {
    "bumpp": "catalog:release",
    "changelogithub": "catalog:release",
    "pkg-pr-new": "catalog:release",
    "tsdown": "catalog:dev",
    "vitest": "catalog:dev"
  },
  "packageManager": "pnpm@10.28.0"
}
