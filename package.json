{
  "name": "gray-matter-es",
  "version": "0.2.1",
  "description": "ES-only gray-matter",
  "keywords": [
    "esm",
    "front-matter",
    "gray-matter",
    "yaml"
  ],
  "homepage": "https://github.com/ryoppippi/gray-matter-es#readme",
  "bugs": {
    "url": "https://github.com/ryoppippi/gray-matter-es/issues"
  },
  "license": "MIT",
  "author": "ryoppippi",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryoppippi/gray-matter-es.git"
  },
  "files": [
    "dist",
    "src"
  ],
  "type": "module",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.mjs",
      "./package.json": "./package.json"
    }
  },
  "scripts": {
    "build": "tsdown",
    "test": "vitest",
    "lint": "pnpm --aggregate-output run '/^lint:/'",
    "lint:oxlint": "oxlint --max-warnings=0 --type-aware --type-check",
    "lint:oxfmt": "oxfmt --no-error-on-unmatched-pattern --check .",
    "lint:knip": "knip",
    "format": "pnpm --no-bail --aggregate-output run '/^format:/'",
    "format:oxlint": "oxlint --max-warnings=0 --type-aware --type-check",
    "format:oxfmt": "oxfmt --no-error-on-unmatched-pattern .",
    "format:knip": "knip --fix --no-exit-code",
    "prepack": "npm pkg delete scripts.preinstall && pnpm run build",
    "prerelease": "pnpm run lint && pnpm run build",
    "release": "bumpp",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@fast-check/vitest": "catalog:dev",
    "@std/yaml": "catalog:jsr",
    "@vitest/coverage-v8": "catalog:dev",
    "bumpp": "catalog:release",
    "changelogithub": "catalog:release",
    "knip": "catalog:dev",
    "publint": "catalog:dev",
    "tsdown": "catalog:dev",
    "unplugin-unused": "catalog:dev",
    "vitest": "catalog:dev"
  },
  "engines": {
    "node": ">=20.19.6"
  },
  "packageManager": "pnpm@10.28.0"
}
